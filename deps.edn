           net.clojars.wkok/openai-clojure     {:mvn/version "0.16.0"}
           org.apache.logging.log4j/log4j-api  {:mvn/version "2.22.1"} ; ToDo: Move logging to :dev.
           org.apache.logging.log4j/log4j-core {:mvn/version "2.22.1"} ; ToDo: Move logging to :dev.
           org.clj-commons/hickory             {:mvn/version "0.7.4"}
